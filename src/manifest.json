{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "version": "1.0.0",
  "description": "__MSG_extDesc__",
  "content_scripts": [
    {
      "matches": [
        "http://www.acfun.cn/v/*",
        "http://www.acfun.cn/bangumi/*"
      ],
      "all_frames": true,
      "js": [
        "dom_gen.js",
        "resizeSensor.js",
        "rc4.js",
        "flv.min.js",
        "acfun_html5.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "*://*/*",
    "storage"
  ],
  "background": {
    "scripts": [
      "script_executer.js"
    ],
    "persistent":true
  },
  "icons": {
    "48": "icon.png"
  },
  "default_locale": "en",
  "web_accessible_resources": [
    "player.html",
    "ABPlayer.css"
  ],
  "minimum_chrome_version": "49.0"
}